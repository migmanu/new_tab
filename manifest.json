{
    "manifest_version": 2,
    "name": "simple nwe tab",
    "version": "1.0.0",
    "description": "simple new tab extension for Chrome",
    
    "chrome_url_overrides": {
        "newtab:": "index.html"
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": ["storage", "unlimitedStorage"],
    "background": {
        "scripts": [".JS/icon.js"],
        "persistent": false
    },
    "minimum_chrome_version": 60
}