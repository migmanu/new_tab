{
    "manifest_version": 2,
    "name": "simple new tab",
    "version": "1.0.0",
    "description": "simple new tab extension for Chrome. Not intended for public use yet",
    
    "chrome_url_overrides": {
        "newtab:": "/index.html"
    },
    "permissions": ["storage", "unlimitedStorage"],
    "background": {
        "scripts": ["/JS/background.js"]
    },
    "minimum_chrome_version": "60"
}